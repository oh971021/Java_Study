
public class Notebook extends Product {
	int battery;
	int volume;
	
	public Notebook() {}
	
	public Notebook(String name, int price, int battery, int volume) {
		super(name, price);
		this.battery = battery;
		this.volume = volume;
	}
	
	@Override
	public void info() {
		super.info();
		System.out.println("배터리는 "+battery+"% 입니다.");
	}
}
